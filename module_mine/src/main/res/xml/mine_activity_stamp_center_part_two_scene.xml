<?xml version="1.0" encoding="utf-8"?>
<MotionScence xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <Transition
        motion:constraintSetEnd="@id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="5000">
<!--        <OnSwipe-->
<!--            motion:dragDirection="dragRight"-->
<!--            motion:touchAnchorId="@+id/button"-->
<!--            motion:touchAnchorSide="right" />-->
        <KeyFrameSet>

            <KeyAttribute
                android:scaleX="0.2"
                android:scaleY="0.2"
                motion:framePosition="65"
                motion:motionTarget="@+id/qa_iv_center_before" />
            <KeyAttribute
                android:alpha="0.1"
                motion:framePosition="65"
                motion:motionTarget="@+id/qa_iv_center_before" />
            <KeyPosition
                motion:framePosition="65"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/qa_iv_center_after"
                motion:pathMotionArc="none"
                motion:percentX="0.757"
                motion:percentY="0.52" />
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="0"
                motion:motionTarget="@+id/qa_iv_center_after" />
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="65"
                motion:motionTarget="@+id/qa_iv_center_after" />
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="100"
                motion:motionTarget="@+id/qa_iv_center_before" />
            <KeyAttribute
                android:alpha="0.1"
                motion:framePosition="67"
                motion:motionTarget="@+id/qa_iv_center_after" />
            <KeyPosition
                motion:framePosition="65"
                motion:keyPositionType="parentRelative"
                motion:motionTarget="@+id/qa_iv_center_before"
                motion:percentX="0.86"
                motion:percentY="0.53" />
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="67"
                motion:motionTarget="@+id/qa_iv_center_before" />
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="0"
                motion:motionTarget="@+id/qa_tv_center_stamp_num_after" />
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="80"
                motion:motionTarget="@+id/qa_tv_center_stamp_num_after" />
            <KeyAttribute
                motion:motionTarget="@+id/mine_center_part_three"
                motion:framePosition="100"
                android:alpha="0.5" />


        </KeyFrameSet>
    </Transition>
    <ConstraintSet android:id="@+id/start">

        <Constraint
            android:id="@+id/qa_iv_center_before"
            android:layout_width="132dp"
            android:layout_height="119dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.86"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.0" />
        <Constraint
            android:id="@+id/qa_iv_center_after"
            android:layout_width="23dp"
            android:layout_height="22dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.76"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.473" />
        <Constraint
            android:id="@+id/qa_tv_center_stamp_num_after"
            android:layout_width="0.001dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.992" />
        <Constraint
            android:id="@+id/mine_center_part_three"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="30dp"/>

    </ConstraintSet>
    <ConstraintSet android:id="@+id/end">

        <Constraint
            android:id="@+id/qa_iv_center_before"
            android:layout_width="132dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.878"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.531"
            motion:transitionEasing="linear" />
        <Constraint
            android:id="@+id/qa_iv_center_after"
            android:layout_width="23dp"
            android:layout_height="22dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.783"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.971" />
        <Constraint
            android:id="@+id/qa_tv_center_stamp_num_after"
            android:layout_width="90dp"
            android:layout_height="28dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.968"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintVertical_bias="0.98" />
        <Constraint
            android:id="@+id/mine_center_part_three"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="100dp"
            android:layout_marginEnd="100dp"
            app:motionProgress="1"/>
    </ConstraintSet>
</MotionScence>